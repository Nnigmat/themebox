{
  "version": 2,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/build",
      "destination": "/api/build"
    }
  ],
  "headers": [
    {
      "source": "/build",
      "headers" : [
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        },
        {
          "key" : "Access-Control-Allow-Methods",
          "value" : "OPTIONS,POST"
        },
        {
          "key" : "Access-Control-Allow-Headers",
          "value" : "Content-Type"
        }
      ]
    }
  ]
}
